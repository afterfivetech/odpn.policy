    </type>
    <type type_id="odpn.profile.events">
      <bound-workflow workflow_id="odpn_events"/>
    </type>
    <type type_id="odpn.profile.membership">
      <bound-workflow workflow_id="odpn_membership"/>
    </type>
 </bindings>
</object>
